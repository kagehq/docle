{
  "name": "docle",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "sdk",
    "packages/react",
    "packages/vue",
    "embed"
  ],
  "type": "module",
  "scripts": {
    "dev": "wrangler dev --local --persist-to .wrangler/state",
    "dev:web": "cd web && npm run dev",
    "start": "npm run dev & npm run dev:web",
    "deploy": "wrangler deploy",
    "deploy:prod": "./deploy.sh",
    "deploy:backend": "./deploy-backend.sh",
    "deploy:frontend": "./deploy-frontend.sh",
    "build:embed": "cd embed && npm install && npm run build",
    "upload:embed": "npm run build:embed && npx wrangler kv:key put --binding=RUNS 'embed.js' --path='embed/dist/embed.js'",
    "format": "prettier -w .",
    "typecheck": "tsc -p ."
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241004.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "wrangler": "^4.45.0",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@cloudflare/sandbox": "^0.4.7",
    "hono": "^4.5.11"
  }
}
